generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model CommunityMonitor {
  id        Int      @id @default(autoincrement())
  subreddit String
  createdAt DateTime @default(now())
}

model AdCampaign {
  id        Int      @id @default(autoincrement())
  name      String
  createdAt DateTime @default(now())
}
