{
  "expo": {
    "name": "DryJets Driver",
    "slug": "dryjets-driver",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "jsEngine": "jsc",
    "developmentClient": {
      "silentLaunch": true
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.dryjets.driver",
      "jsEngine": "jsc",
      "config": {
        "googleMapsApiKey": "YOUR_IOS_GOOGLE_MAPS_API_KEY"
      },
      "infoPlist": {
        "NSLocationAlwaysAndWhenInUseUsageDescription": "DryJets Driver needs your location to accept deliveries and navigate to customers.",
        "NSLocationWhenInUseUsageDescription": "DryJets Driver needs your location to accept deliveries and navigate to customers.",
        "NSCameraUsageDescription": "DryJets Driver needs camera access for proof of delivery photos.",
        "NSPhotoLibraryUsageDescription": "DryJets Driver needs access to your photos for proof of delivery."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.dryjets.driver",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "POST_NOTIFICATIONS"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "YOUR_ANDROID_GOOGLE_MAPS_API_KEY"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-location",
      "expo-image-picker",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#10b981",
          "sounds": []
        }
      ]
    ],
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#10b981",
      "androidMode": "default",
      "androidCollapsedTitle": "DryJets Driver"
    },
    "scheme": "dryjets-driver",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "your-project-id"
      }
    }
  }
}
