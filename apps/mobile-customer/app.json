{
  "expo": {
    "name": "DryJets Customer",
    "slug": "dryjets-customer",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dryjets.customer",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "DryJets needs your location to find nearby cleaners and track deliveries.",
        "NSCameraUsageDescription": "DryJets needs camera access to take photos of items.",
        "NSPhotoLibraryUsageDescription": "DryJets needs access to your photos to upload item images."
      }
    },
    "android": {
      "package": "com.dryjets.customer",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE"
      ]
    },
    "plugins": [
      "expo-router",
      "expo-location",
      "expo-image-picker",
      "expo-asset"
    ],
    "scheme": "dryjets",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "your-project-id"
      }
    }
  }
}
